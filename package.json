{
  "name": "gedrat-grid-table",
  "version": "0.0.1",
  "description": "TableLess Css-grid table",
  "private": "false",
  "keywords": [
    "TableLess grid",
    "react",
    "component",
    "components",
    "ecosystem-react",
    "react-component"
  ],
  "homepage": "https://github.com/jonasgedrat/gedrat-grid-table",
  "bugs": {
    "url": "https://github.com/jonasgedrat/gedrat-grid-table/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Jonas Gedrat",
    "email": "jonasgedrat@outlook.com"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jonasgedrat/gedrat-grid-table"
  },
  "scripts": {
    "dev": "pnpm run --filter web dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,css,scss}\"",
    "clean": "pnpm clean:node-modules && pnpm clean:dist &&  pnpm install",
    "clean:node-modules": "npx rimraf --glob **/node_modules",
    "clean:dist": "npx rimraf --glob **/dist",
    "lint": "pnpm lint:pkg && pnpm lint:web",
    "lint:web": "eslint -c .eslintrc.json ./web/**/*.{ts,tsx}",
    "lint:pkg": "eslint -c .eslintrc.json ./packages/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix -c .eslintrc.json ./packages/**/*.{ts,tsx}",
    "lint:web-fix": "eslint --fix -c .eslintrc.json ./web/**/*.{ts,tsx}"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "commitlint-plugin-function-rules": "^4.0.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  },
  "engines": {
    "node": ">=20.x",
    "pnpm": ">=9.x"
  }
}
